        <div class="paragraphs col-md-9">





          <div class="row">

            <% events.forEach(element => { %>
            <% if (element.image_path) { %>
            <div class="col-md-4">
              <a href="/evenementen/<%= element.EventID  %>" title="<%= element.Name  %>" class="img-16by9-cover img-headline">
                <img src="<%= element.image_path  %>" alt="<%= element.Name  %>" class="img-responsive">
              </a>
            </div>
            <% } %>
            <div class=" <%= element.image_path ? 'ol-md-8': 'ol-md-10' %>">
              <div class="event-header">
                <h3><a href="/evenementen/<%= element.EventID  %>" title="<%= element.Name  %>"><%= element.Name  %></a></h3>
                <ul class="list-unstyled list-inline text-muted small meta-list">
                  <!-- <li><span class="fa fa-calendar"></span> 28 sep. 2023 16:00 - 19:30</li> -->

                  <% let start = new Date(element.StartDateTime); %>
                  <% let end = new Date(element.EndDateTime); %>
                  <% if(start.toDateString() === end.toDateString()) { %>
                  <li><span class="fa fa-calendar"></span><%= start.toLocaleString('nl-NL', {day: '2-digit', month: 'short', year: 'numeric', hour: '2-digit', minute: '2-digit'}) %> - <%= end.toLocaleString('nl-NL', {hour: '2-digit', minute: '2-digit'}) %></li>
                  <% } else { %>
                  <li><span class="fa fa-calendar"></span><%= start.toLocaleString('nl-NL', {day: 'numeric'}) %> - <%= end.toLocaleString('nl-NL', {day: 'numeric', month: 'short', year: 'numeric'}) %></li>
                  <% } %>


                  <li><span class="fa fa-tag"></span> <%= element.EventCategoryName  %></li>
                  <li><span class="fa fa-group"></span> <%= element.Attendees  %> deelnemers</li>

                  <% if (element.Attendees !== element.AvailableQuantity) { %>
                  <li class="text-success meta-tag is-success">Tickets te koop</li>
                  <% } else {%>
                  <li class="text-primary meta-tag is-primary">Uitverkocht</li>
                  <% }%>

                  <!-- <li class="text-success meta-tag is-success">Aanmelding open</li> -->

                </ul>
              </div>
              <p><%= element.Notes  %></p>
              <p>
                <a class="btn btn-link" href="/evenementen/<%= element.EventID  %>"><i class="fa fa-fw fa-chevron-circle-right"></i> Meer informatie</a>
              </p>
            </div>
            <div class="col-md-12">
              <hr>
            </div>
